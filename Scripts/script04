-- Table: public.log_pesquisa_usuario

-- DROP TABLE public.log_pesquisa_usuario;

CREATE TABLE public.log_pesquisa_usuario
(
    id_pesquisa integer NOT NULL DEFAULT nextval('log_pesquisa_usuario_id_pesquisa_seq'::regclass),
    id_usuario integer,
    ds_log text COLLATE pg_catalog."default",
    dt_atualizacao timestamp without time zone,
    CONSTRAINT log_pesquisa_usuario_pkey PRIMARY KEY (id_pesquisa),
    CONSTRAINT log_pesquisa_usuario FOREIGN KEY (id_usuario)
        REFERENCES public.usuario (id_usuario) MATCH SIMPLE
        ON UPDATE NO ACTION
        ON DELETE NO ACTION
)
WITH (
    OIDS = FALSE
)
TABLESPACE pg_default;

ALTER TABLE public.log_pesquisa_usuario
    OWNER to postgres;
